#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/vite@7.1.9/node_modules/vite/bin/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/vite@7.1.9/node_modules/vite/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/vite@7.1.9/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/node_modules:/mnt/d/Teach2Give/Codes/Week 3/node_modules:/mnt/d/Teach2Give/Codes/node_modules:/mnt/d/Teach2Give/node_modules:/mnt/d/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-cart/ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/vite@7.1.9/node_modules/vite/bin/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/vite@7.1.9/node_modules/vite/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/vite@7.1.9/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules/.pnpm/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/TS/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-Cart/node_modules:/mnt/d/Teach2Give/Codes/Week 3/node_modules:/mnt/d/Teach2Give/Codes/node_modules:/mnt/d/Teach2Give/node_modules:/mnt/d/node_modules:/mnt/d/Teach2Give/Codes/Week 3/Product-cart/ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
